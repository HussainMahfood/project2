<h2> <%= car.plateId %> </h2>

<div>
    Model: <%= car.model %>
</div>

<div>
    Make: <%= car.make %>
</div>

<div>
    Manufacture Year: <%= car.manufactureYear %>
</div>

<div>
    Insuarnce Date: <%= car.insuranceDate %>
</div>

<div>
    Value: <%= car.value %>
</div>

<div>
    Is New Car?: <%= car.isNewCar %>
</div>

<div>
    Created At: <%= moment(car.createdAt).fromNow() %>
</div>

<div>
    Updated At: <%= moment(car.updatedAt).fromNow() %>
</div>


<% car.user.forEach(function(user){ %>
    <hr>
  
    <div class="user"> 
        <div>First Name: <%= user.firstName %></div>
        <div>Last Name: <%= user.lastName %></div>
        <div>Email Address: <%= user.emailAddress %></div>
        <div>Phone Number: <%= user.phoneNumber %></div>
        <div>User Created on: <%= moment(user.createdAt).fromNow() %></div>
    </div>
<% }) %>