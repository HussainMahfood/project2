<h2> <%= car.plateId %> </h2>

<div>
    Model: <%= car.model %>
</div>

<div>
    Make: <%= car.make %>
</div>

<div>
    Manufacture Year: <%= car.manufactureYear %>
</div>

<div>
    Insuarnce Date: <%= moment(car.insuranceDate).format('DD-MM-YYYY') %>
</div>

<div>
    Value: <%= car.value %>
</div>

<div>
    Is New Car?: <%= car.isNewCar %>
</div>

<div>
    Created At: <%= moment(car.createdAt).fromNow() %>
</div>

<div>
    Updated At: <%= moment(car.updatedAt).fromNow() %>
</div>

